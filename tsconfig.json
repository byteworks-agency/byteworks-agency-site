{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "jsx": "preserve",
    "types": [
      "astro/client"
    ],
    "ignoreDeprecations": "6.0"
  },
  "include": [
    "src",
    "env.d.ts"
  ],
  "exclude": [
    "dist",
    "node_modules",
    ".astro"
  ]
}